{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstname": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "userEmail": {
      "type": "email",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "total": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "payment_required",
        "payed",
        "refunded",
        "cancelled"
      ],
      "default": "payment_required"
    },
    "country": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "postal_code": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "code": {
      "type": "string"
    },
    "orders": {
      "type": "json"
    }
  }
}
